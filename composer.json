{
    "name": "michiel/geocoder",
    "description": "Geocoder package for PHP that offers a user friendly interface for Geocoder services (such as Google Maps). The package is framework agnostic, but includes a ServiceProvider for Laravel.",
    "license": "MIT",
    "keywords": ["geocoding","geocoder","geocode","google","maps","api","longitude","latitude","location"],
    "authors": [
        {
            "name": "Michiel van der Geest",
            "email": "michiel@mileswebmedia.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "guzzlehttp/guzzle": "~5.0"
    },
    "autoload": {
        "psr-0": {
            "Michiel\\Geocoder\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
